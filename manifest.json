{
    "name": "Class Schedule",
    "version": "1.0",
    "manifest_version": 2,
    "description": "This extension is used to see your class schedule quickly",
    "browser_action": {
        "default_popup": "/Client/src/login.html",
        "popup": "/Client/src/login.html"
    },
    "permissions": [
        "storage", 
        "tabs",
        "https://quickview-classschedule.herokuapp.com/login",
        "http://localhost:3000/login"
    ]
}